    .wrapper
      .page-header.page-header-small
        .page-header-image{"data-parallax" => "true", style: "background-image:  url('../assets/img/bg45.jpg');"}
      .main
        .contact-content
          .container
            .row
              .col-md-5.ml-auto.mr-auto
                %h2.title Send us a message
                %p.description
                  You can contact us with anything related to our Products. We'll get in touch with you as soon as possible.
                  %br/
                  %br/
                %form#contact-form{method: "post", role: "form"}
                  %label Your name
                  .input-group
                    .input-group-prepend
                      %span.input-group-text
                        %i.now-ui-icons.users_circle-08
                    %input.form-control{"aria-label" => "Your Name...", placeholder: "Your Name...", type: "text"}/
                  %label Email address
                  .input-group
                    .input-group-prepend
                      %span.input-group-text
                        %i.now-ui-icons.ui-1_email-85
                    %input.form-control{"aria-label" => "Email Here...", placeholder: "Email Here...", type: "email"}/
                  %label Phone
                  .input-group
                    .input-group-prepend
                      %span.input-group-text
                        %i.now-ui-icons.tech_mobile
                    %input.form-control{placeholder: "Number Here...", type: "text"}/
                  .form-group
                    %label Your message
                    %textarea#message.form-control{name: "message", rows: "6"}
                  .submit.text-center
                    %input.btn.btn-primary.btn-raised.btn-round{type: "submit", value: "Contact Us"}/
              .col-md-5.ml-auto.mr-auto
                .info.info-horizontal.mt-5
                  .icon.icon-primary
                    %i.now-ui-icons.location_pin
                  .description
                    %h4.info-title Find us at the office
                    %p
                      Bld Mihail Kogalniceanu, nr. 8,
                      %br/
                      7652 Bucharest,
                      %br/
                      Romania
                .info.info-horizontal
                  .icon.icon-primary
